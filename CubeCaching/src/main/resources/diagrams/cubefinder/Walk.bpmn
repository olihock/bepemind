  <process id="walk" name="Walk" isExecutable="true">
    <startEvent id="start" name="Start"></startEvent>
    <endEvent id="end" name="End"></endEvent>
    <serviceTask id="rotateLeftMotor" name="Rotate Left Motor" activiti:class="com.videaps.mindstorms.ral.motor.RotateBase">
      <extensionElements>
        <activiti:field name="immediateReturn">
          <activiti:expression><![CDATA[${true}]]></activiti:expression>
        </activiti:field>
        <activiti:field name="angle">
          <activiti:expression><![CDATA[${ ( distance / 10 ) * 360 }]]></activiti:expression>
        </activiti:field>
        <activiti:field name="port">
          <activiti:string><![CDATA[B]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="rotateRightMotor" name="Rotate Right Motor" activiti:class="com.videaps.mindstorms.ral.motor.RotateBase">
